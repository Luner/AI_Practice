	}
	
	public JPanel getPanel() {
		return this.panel;
	}
	
	public void gui() {
		
		
		panel = new JPanel();
		panel.setBackground(Color.LIGHT_GRAY);
		
		b1 = new JButton("Graph");
		lab = new JLabel("Y");

		lab = new JLabel("Y");
		
		panel.add(b1);
