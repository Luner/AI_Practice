import javax.swing.*;
import java.awt.*;

public class Graph {

	private JFrame f;
	private JPanel p;
	private JButton b1;
	private JLabel lab;
	
	public Graph() {
		gui();
	}
	
	public void gui() {
		
		f = new JFrame("Linear Regression");
		f.setVisible(true);
		f.setSize(800, 600);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		p = new JPanel();
		p.setBackground(Color.LIGHT_GRAY);
		
		b1 = new JButton("Graph");
		b1.setAlignmentX(100);
		lab = new JLabel("Y");
		
		p.add(b1);
		p.add(lab);
		f.add(p);
	}
	
	public static void main (String[] args) {
		new Graph();
	}
	
	
}
